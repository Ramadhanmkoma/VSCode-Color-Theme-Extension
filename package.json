{
  "name": "techswahili",
  "displayName": "TechSwahili Theme",
  "description": "Color Theme for VScode, made with love.",
  "version": "1.0.1",
  "publisher": "Ramadhanmkoma",
  "engines": {
    "vscode": "^1.79.0"
  },
  "categories": [
    "Themes"
  ],
  "contributes": {
    "themes": [
      {
        "label": "TechSwahili Theme",
        "uiTheme": "vs-dark",
        "path": "./themes/TechSwahili Theme-color-theme.json"
      },
      {
        "label": "TechSwahili Light",
        "uiTheme": "vs",
        "path": "./themes/techswahili-light.json",
        "isDark": false
      },
      {
        "label": "TechSwahili Dark -- No Italics",
        "uiTheme": "vs-dark",
        "path": "./themes/techswahili-dark-no-italics.json",
        "isDark": true
      },
      {
        "label": "Winter Is Back -- TechSwahili Dark",
        "uiTheme": "vs-dark",
        "path": "./themes/techswahili-dark-default.json",
        "isDark": true
      },
      {
        "label": "Winter Is Back -- No Italics -- TechSwahili Dark",
        "uiTheme": "vs-dark",
        "path": "./themes/techswahili-dark-default-no-italics.json",
        "isDark": true
      }
    ]
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Ramadhanmkoma/VSCode-Color-Theme-Extension.git"
  },
  "keywords": [
    "techswahili",
    "theme",
    "color",
    "Ramadhanmkoma",
    "Ramadhan",
    "techswahili theme",
    "ramadhan mkoma",
    "ramadhan mohammed mkoma",
    "dark theme",
    "winter is back",
    "winter is back theme",
    "winter is back color theme",
    "color theme",
    "italic theme",
    "italics theme",
    "techswahili color theme"
  ],
  "icon": "icon.png",
  "license": "MIT",
  "bugs": {
    "url": ""
  }
}
